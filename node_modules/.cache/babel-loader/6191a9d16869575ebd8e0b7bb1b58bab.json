            })\n            .catch((er) => console.log(er));\n    },\n\n    GET_TEACHER({ commit }, userId) {\n        return http.getTeacher(userId)\n            .then(({ data }) => {\n                const teacher = new Teacher(data);\n                commit('SET_TEACHER', teacher);\n            })\n            .catch((er) => console.log(er));\n    },\n};"]}]}